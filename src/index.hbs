                $('#homecontent').css('display', 'block');
            }
        }

        $('<img/>').attr('src', bg).load(function() {
            $(this).remove(); // prevent memory leaks as @benweet suggested
            changeBg();
        });

        new Vivus('igemuoftlogo', {
            duration: 100
        }, changeBg);
    })();
</script>
